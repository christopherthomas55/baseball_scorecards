            <script type="text/javascript">
                (function(window) {
                    'use strict';
            
                    // Hack to clear the props not being cleared in s.clearVars().
                    var CLEAR_VARS = [
                        'eVar13', 'eVar16', 'eVar19', 'eVar28', 'eVar49',
                        'eVar53', 'prop54', 'prop12', 'prop13', 'prop14',
                        'prop25', 'prop26', 'prop27', 'prop28', 'prop29',
